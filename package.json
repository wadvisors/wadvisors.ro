{
  "name": "bknd-react-router-cloudflare",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build:staging": "react-router build --mode staging",
    "build:production": "react-router build --mode production",
    "preview": "npm run build && vite preview",
    "cf-typegen": "wrangler types",
    "bknd": "npx tsx node_modules/.bin/bknd",
    "bknd:staging": "CLOUDFLARE_ENV=staging npx tsx node_modules/.bin/bknd",
    "bknd:production": "CLOUDFLARE_ENV=production npx tsx node_modules/.bin/bknd",
    "deploy:staging": "CLOUDFLARE_ENV=staging npm run build:staging && npm run bknd:staging -- sync --force && wrangler deploy --var WORKERS_CI_BRANCH:$WORKERS_CI_BRANCH --var WORKERS_CI_COMMIT_SHA:$WORKERS_CI_COMMIT_SHA",
    "deploy:production": "CLOUDFLARE_ENV=production npm run build:production && npm run bknd:production -- sync --force && wrangler deploy --var WORKERS_CI_BRANCH:$WORKERS_CI_BRANCH --var WORKERS_CI_COMMIT_SHA:$WORKERS_CI_COMMIT_SHA",
    "typegen": "npm run cf-typegen && react-router typegen",
    "predev": "npm run typegen",
    "typecheck": "npm run typegen && tsc"
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.8.0",
    "bknd": "^0.18.0-rc.2",
    "isbot": "^5.1.29",
    "lucide-react": "^0.544.0",
    "markdown-to-jsx": "^7.7.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "tailwindcss-motion": "^1.1.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.2",
    "@hono/vite-dev-server": "^0.20.1",
    "@react-router/dev": "^7.8.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "react-router-devtools": "^5.0.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.33.1"
  }
}
